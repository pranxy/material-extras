/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "projects",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "paths": {
      "@pranxy/material-extras/components/*": ["extras/components/*"],
      "@pranxy/material-extras/cdk/*": ["extras/cdk/*"],
      "@pranxy/extras-dayjs-adapter": ["extras-dayjs-adapter/extras-dayjs-adapter"],
      "@pranxy/extras-dayjs-adapter/*": ["extras-dayjs-adapter/extras-dayjs-adapter/*"],
    },
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  }
}
