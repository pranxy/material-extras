{
  "name": "material-extras",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=docs",
    "dev": "ng serve --project=dev-app",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "build:lib": "ng build extras --configuration production",
    "deploy": "ng build docs --configuration production --base-href=/material-extras/",
    "publish": "npm run build:lib && cp README.md dist/extras && cd dist/extras && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^13.3.3",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^13.3.3",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^11.2.0",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^14.2.2",
    "raw-loader": "^4.0.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.3"
  }
}
