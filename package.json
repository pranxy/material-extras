{
  "name": "material-extras",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=docs",
    "dev": "ng serve --project=dev-app",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "build:lib": "ng build extras --configuration production",
    "deploy": "ng build docs --configuration production --base-href=/material-extras/",
    "publish": "npm run build:lib && cp README.md dist/extras && cd dist/extras && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^11.2.0",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^15.2.2",
    "raw-loader": "^4.0.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  }
}